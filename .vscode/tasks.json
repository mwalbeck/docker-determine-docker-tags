{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "docker-build",
			"label": "docker-build: alpine",
			"dockerBuild": {
				"tag": "dockerdeterminedockertags:alpine",
				"dockerfile": "${workspaceFolder}/Dockerfile.alpine",
				"context": "${workspaceFolder}",
				"pull": true
			}
		},
		{
			"type": "docker-run",
			"label": "docker-run: alpine",
			"dockerRun": {
				"image": "dockerdeterminedockertags:alpine",
				"remove": true
			},
			"dependsOn": [
				"docker-build"
			],
		},
		{
			"type": "docker-build",
			"label": "docker-build: debian",
			"dockerBuild": {
				"tag": "dockerdeterminedockertags:debian",
				"dockerfile": "${workspaceFolder}/Dockerfile.debian",
				"context": "${workspaceFolder}",
				"pull": true
			}
		},
		{
			"type": "docker-run",
			"label": "docker-run: debian",
			"dockerRun": {
				"image": "dockerdeterminedockertags:debian",
				"remove": true
			},
			"dependsOn": [
				"docker-build"
			],
		}
	]
}
